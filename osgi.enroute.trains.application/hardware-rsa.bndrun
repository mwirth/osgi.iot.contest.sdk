x-runpath: biz.aQute.remote.launcher
x-runremote: pi-1; \
	host=192.168.99.11; \
	jdb=1044; \
	shell=-1
	
-runtrace: false

-include: ~paremus-rsa.bnd
-resolve.effective: active;skip:="osgi.service"

