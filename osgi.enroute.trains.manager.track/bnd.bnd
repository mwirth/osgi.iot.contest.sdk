#
# OSGI ENROUTE TRAINS TRACK MANAGER EXAMPLE PROVIDER BUNDLE 
#


Bundle-Version: 2.0.0.${tstamp}
Bundle-Description:  \
	This is OSGI ENROUTE TRAINS Example Track Manager project.
Export-Package: \
	osgi.enroute.trains.track.api,\
	osgi.enroute.trains.segment.api
Private-Package: osgi.enroute.trains.track.manager
	

Conditional-Package: aQute.lib*

-buildpath: \
	osgi.enroute.base.api;version=1.0,\
	biz.aQute.bndlib,\
	osgi.enroute.trains.api;version=latest,\
	osgi.enroute.mqtt.provider;version=latest,\
	org.apache.felix.converter,\
	osgi.enroute.trains.util;version=latest,\
	org.osgi.util.pushstream

-testpath: \
	osgi.enroute.junit.wrapper

-includeresource: {readme.md}


-runrequires:  \
	osgi.identity;filter:='(osgi.identity=osgi.enroute.trains.track.manager.example.provider)'
	
-runbundles: \
	${error;Resolve first}
